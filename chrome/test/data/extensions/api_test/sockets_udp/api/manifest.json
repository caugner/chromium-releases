{
  "manifest_version": 2,
  "name": "chrome.sockets.udp",
  "version": "0.1",
  "description": "end-to-end browser test for chrome.sockets.udp API",
  "app": {
    "background": {
      "scripts": ["multicast.js", "background.js"]
    }
  },
  "permissions": [
    { "socket": ["udp-send-to", "udp-bind", "udp-multicast-membership"] },
    "sockets.udp"
  ]
}
