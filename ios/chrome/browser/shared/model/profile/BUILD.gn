source_set("profile") {
  sources = [
    "profile_attributes_storage_ios.cc",
    "profile_attributes_storage_ios.h",
    "profile_ios.h",
    "profile_ios_forward.h",
    "profile_manager_ios.h",
    "profile_manager_observer_ios.h",
  ]

  deps = [
    "//base",
    "//base:i18n",
    "//components/prefs",
    "//ios/chrome/browser/shared/model/prefs:pref_names",
